# Strategic Integration Service Environment Variables
# Copy this to .env and configure for your environment

# Jira Configuration
JIRA_API_TOKEN=your_jira_api_token_here
JIRA_EMAIL=user@company.com
JIRA_BASE_URL=https://your-domain.atlassian.net

# Application Configuration
DEBUG=true
LOG_LEVEL=INFO

# Engineering Team Configuration
L2_DIVISION_FILTER="Engineering Division"
L2_CUSTOM_FIELD_PRIORITY="cf[18272]"

# Output Directories
OUTPUT_BASE_DIR=workspace/current-initiatives
REPORT_OUTPUT_DIR=executive/reports

# Security Settings
MASK_PII=true
ENCRYPT_SENSITIVE_DATA=true
AUDIT_LOG_ENABLED=true

# Performance Settings
ENABLE_CACHING=true
CACHE_TTL_SECONDS=300
PARALLEL_REQUESTS=5
BATCH_SIZE=100
ENABLE_MEMORY_CACHE=true
ENABLE_FILE_CACHE=true
ENABLE_REDIS_CACHE=false
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
# REDIS_PASSWORD=your_redis_password_here
PERFORMANCE_MONITORING=true
